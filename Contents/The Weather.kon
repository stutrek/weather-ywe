<?xml version="1.0" encoding="macintosh"?><widget version="1.0" minimumVersion="2.1"><debug>verbose</debug><!--	mini Weather	Code & Images: Arlo Rose	This Widget grabs the latest current weather data from weather.com and builds	it into a nice little iconic representation.		For more information about the services weather.com offers, visit their	website at: <http://www.weather.com>	(c) 2003 - 2004 Arlo Rose & Perry Clarke-->	<window title="mini Weather">		<name>main_window</name>		<width>440</width>		<height>297</height>		<alignment>left</alignment>		<opacity>255</opacity>		<visible>0</visible>		<shadow>0</shadow>	</window>		<image src="Resources/Map.jpg">		<name>map</name>		<width>440</width>		<height>297</height>		<alignment>left</alignment>		<opacity>255</opacity>		<zOrder>10</zOrder>		<onMouseUp><!--					main_window.shadow = 0;			main_window.recalcShadow();			animator.start( fade_out );  						-->		</onMouseUp>		</image>	<!--	<image src="Resources/Left.png">		<name>backLeft</name>		<hOffset>21</hOffset>		<vOffset>14</vOffset>	</image>	<image src="Resources/Middle.png">		<name>backMiddle</name>		<hOffset>60</hOffset>		<vOffset>14</vOffset>	</image>	<image src="Resources/Right.png">		<name>backRight</name>		<hOffset>62</hOffset>		<vOffset>14</vOffset>	</image>-->	<text>		<name>degreeSymShadow</name>		<font>Futura</font>		<style>Narrow</style>		<size>14</size>		<data>¡</data>		<color>#000000</color>		<alignment>right</alignment>		<hOffset>50</hOffset>		<vOffset>31</vOffset>		<opacity>0%</opacity>		<zOrder>8</zOrder>	</text>	<text>		<name>degreeSym</name>		<font>Futura</font>		<style>Narrow</style>		<size>14</size>		<data>¡</data>		<color>#000000</color>		<alignment>right</alignment>		<hOffset>49</hOffset>		<vOffset>30</vOffset>		<zOrder>9</zOrder>		<onMouseUp><!--		updateWeather();  			getMap();-->		</onMouseUp>		<tooltip>Update Weather</tooltip>	</text>		<text>		<name>theTempShadow</name>		<font>Futura</font>		<style>Condensed</style>		<size>21</size>		<data>?</data>		<color>#000000</color>		<alignment>right</alignment>		<hOffset>45</hOffset>		<vOffset>35</vOffset>		<opacity>0%</opacity>		<zOrder>8</zOrder>	</text>	<text>		<name>theTemp</name>		<font>Futura</font>		<style>Condensed</style>		<size>21</size>		<data>?</data>		<color>#000000</color>		<alignment>right</alignment>		<hOffset>44</hOffset>		<vOffset>34</vOffset>		<zOrder>9</zOrder>		<onMouseUp><!--					print( animator.kEaseOut );			animator.start( fade_in );			//map.fade(0, 255, 1);			-->		</onMouseUp>		<tooltip>Show Map</tooltip>	</text><!--	<text>		<name>theCityShadow</name>		<font>Futura</font>		<style>bold</style>	    <data>Unknown</data>		<size>12</size>		<color>#000000</color>		<alignment>right</alignment>		<hOffset>92</hOffset>		<vOffset>52</vOffset>		<opacity>50%</opacity>	</text>	<text>		<name>theCity</name>		<font>Futura</font>		<style>bold</style>		<data>Unknown</data>		<size>12</size>		<color>#ffffff</color>		<alignment>right</alignment>		<hOffset>91</hOffset>		<vOffset>51</vOffset>	</text>-->	<image src="Resources/Sun.png">		<name>Sun</name>		<alignment>left</alignment>		<hOffset>0</hOffset>		<vOffset>0</vOffset>		<zOrder>0</zOrder>	</image>	<image src="Resources/Sleet.png">		<name>waterObject</name>		<alignment>left</alignment>		<hOffset>8</hOffset>		<vOffset>45</vOffset>		<opacity>0</opacity>		<zOrder>1</zOrder>	</image>	<image src="Resources/Smaller Clouds.png">		<name>Cloud</name>		<alignment>left</alignment>		<hOffset>26</hOffset>		<vOffset>34</vOffset>		<zOrder>2</zOrder>	</image>					<action trigger="onLoad" file="weather.js"/>		<action trigger="onTimer" interval="9">		log('hi');		updateWeather();	</action>	<action trigger="onWakeFromSleep">		sleep(7000);		updateWeather();	</action>	<action trigger="onWillChangePreferences">		oldUserCity = preferences.userDisplayPref.value;	</action>	<action trigger="onPreferencesChanged">		moveIcon();		chooseCity();	</action>	<preference name="userDisplayPref">		<title>Location:</title>		<type>text</type>		<defaultValue>Syracuse, NY</defaultValue>		<description>You can enter a US zip code, your City, State, or City, Country.</description>	</preference>	<preference name="unitsPref">		<title>Use Metric Units</title>		<type>checkbox</type>		<defaultValue>0</defaultValue>		<description>Check this box if you would like your weather displayed in degrees Celsius rather than degrees Fahrenheit.</description>	</preference>	<preference name="showToolTips">		<title>Show Tooltips</title>		<type>checkbox</type>		<defaultValue>1</defaultValue>		<description>Check this box if you would like detailed information when hovering over the weather icons.</description>	</preference>	<preference name="cityValPref">		<hidden>true</hidden>		<type>text</type>		<defaultValue>USCA0830</defaultValue>		<description>Check this box if you would like to show tooltips that give weather details.</description>	</preference>		<preference name="mapURL">		<title>Copy and paste the location of your map from weather.com.</title>		<type>text</type>		<defaultValue>http://image.weather.com/web/radar/us_roc_closeradar_medium_usen.jpg</defaultValue>		<description>This should start with http://.</description>	</preference>		<preference name="hPlacePref">		<title>Horizontal Placement of weather icon.</title>		<type>slider</type>		<tickLabel>Left</tickLabel>		<tickLabel>Center</tickLabel>		<tickLabel>Right</tickLabel>		<minLength>0</minLength>		<maxLength>375</maxLength>		<defaultValue>350</defaultValue>	</preference>		<preference name="vPlacePref">		<title>Vertical Placement of weather icon.</title>		<type>slider</type>		<tickLabel>Top</tickLabel>		<tickLabel>Middle</tickLabel>		<tickLabel>Bottom</tickLabel>		<minLength>0</minLength>		<maxLength>245</maxLength>		<defaultValue>0</defaultValue>	</preference>		<about-box>		<image>Resources/About.png</image>	</about-box></widget>